<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cpu11398_local.threadmanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service android:name=".ThreadTypes.MyService" />
        <service android:name=".ThreadTypes.MyIntentService" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ThreadTypes.ActivityThread"
            android:label="Thread" />
        <activity
            android:name=".ThreadTypes.ActivityAsyncTask"
            android:label="AsyncTask" />
        <activity
            android:name=".ThreadTypes.ActivityService"
            android:label="Service" />
        <activity
            android:name=".ThreadTypes.ActivityIntentService"
            android:label="IntentService"/>
        <activity
            android:name=".ThreadTypes.ActivityRxJava"
            android:label="RxJava"/>
    </application>

</manifest>