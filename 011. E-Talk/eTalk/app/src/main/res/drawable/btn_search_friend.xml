<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >
            <gradient
                android:angle="0"
                android:startColor="@color/colorETalkClickDark"
                android:endColor="@color/colorETalkClickLight"/>
            <corners
                android:radius="50dp" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >
            <gradient
                android:startColor="@color/colorETalkDark"
                android:endColor="@color/colorETalkLight"/>
            <corners
                android:radius="50dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >
            <solid
                android:color="@color/colorETalkDisable"/>
            <corners
                android:radius="50dp" />
        </shape>
    </item>
</selector>