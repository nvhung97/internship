<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@id/lyt_message_group_sound_me_row"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <FrameLayout
        android:id="@id/lyt_message_group_sound_me_bound_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dp">
        <android.support.constraint.ConstraintLayout
            android:id="@id/lyt_message_group_sound_me_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="25dp">
            <com.example.cpu11398_local.etalk.presentation.custom.ClockView
                android:id="@id/lyt_message_group_sound_me_data"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="125dp"
                android:paddingTop="15dp"
                android:paddingRight="10dp"
                android:paddingBottom="15dp"
                android:textSize="16sp"
                android:text="--:--"
                android:textColor="@color/colorBlack"
                app:layout_constraintRight_toRightOf="parent"
                android:background="@drawable/message_me_background"/>
            <ImageButton
                android:id="@id/lyt_message_group_sound_me_stop"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/ic_stop"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageButton
                android:id="@id/lyt_message_group_sound_me_play"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/ic_play"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <ProgressBar
                android:id="@id/lyt_message_group_sound_me_sending"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@id/lyt_message_group_sound_me_equalizer"
                android:layout_width="55dp"
                android:layout_height="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="50dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"/>
            <TextView
                android:id="@id/lyt_message_group_sound_me_time"
                android:layout_width="wrap_content"
                android:layout_height="18dp"
                android:layout_marginTop="5dp"
                android:paddingLeft="10dp"
                android:paddingBottom="2dp"
                android:paddingRight="10dp"
                android:textSize="12sp"
                android:textColor="@color/colorWhite"
                android:background="@drawable/img_rounded_group_number"
                app:layout_constraintLeft_toLeftOf="@id/lyt_message_group_sound_me_data"
                app:layout_constraintTop_toBottomOf="@id/lyt_message_group_sound_me_data"/>
        </android.support.constraint.ConstraintLayout>
        <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
            android:id="@id/lyt_message_group_sound_me_status"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="23dp"
            android:layout_gravity="bottom|right"/>
    </FrameLayout>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_sound_me_seen1"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_sound_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_sound_me_seen2"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_sound_me_seen2"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_sound_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_sound_me_seen3"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_sound_me_seen3"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_sound_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_sound_me_seen4"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_sound_me_seen4"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_sound_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_sound_me_seen5"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_sound_me_seen5"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_sound_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_sound_me_seen6"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_sound_me_seen6"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_sound_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_sound_me_seen7"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_sound_me_seen7"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_sound_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_sound_me_seen8"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_sound_me_seen8"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_sound_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_sound_me_seen9"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_sound_me_seen9"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_sound_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_sound_me_more"/>
    <TextView
        android:id="@+id/lyt_message_group_sound_me_more"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:textSize="10sp"
        android:gravity="center"
        android:background="@drawable/img_rounded_group_number"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_sound_me_bound_content"
        app:layout_constraintRight_toRightOf="parent"/>
</android.support.constraint.ConstraintLayout>