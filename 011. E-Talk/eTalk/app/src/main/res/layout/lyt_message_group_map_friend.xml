<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@id/lyt_message_group_map_friend_row"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@id/lyt_message_group_map_friend_name"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginLeft="50dp"
        android:textSize="13sp"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <FrameLayout
        android:id="@id/lyt_message_group_map_friend_bound_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="75dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_name">
        <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
            android:id="@id/lyt_message_group_map_friend_avatar"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"/>
        <android.support.constraint.ConstraintLayout
            android:id="@id/lyt_message_group_map_friend_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:layout_gravity="left">
            <com.example.cpu11398_local.etalk.presentation.custom.RoundedMapView
                android:id="@id/lyt_message_group_map_friend_data"
                android:layout_width="match_parent"
                android:layout_height="200dp"/>
            <View
                android:id="@+id/align"
                android:layout_width="1dp"
                android:layout_height="1dp"
                app:layout_constraintTop_toTopOf="@id/lyt_message_group_map_friend_data"
                app:layout_constraintLeft_toLeftOf="@id/lyt_message_group_map_friend_data"
                app:layout_constraintRight_toRightOf="@id/lyt_message_group_map_friend_data"
                app:layout_constraintBottom_toBottomOf="@id/lyt_message_group_map_friend_data"/>
            <ImageView
                android:id="@id/lyt_message_group_map_friend_marker"
                android:layout_width="33dp"
                android:layout_height="40.5dp"
                android:src="@drawable/ic_avatar_marker"
                app:layout_constraintLeft_toLeftOf="@id/lyt_message_group_map_friend_data"
                app:layout_constraintRight_toRightOf="@id/lyt_message_group_map_friend_data"
                app:layout_constraintBottom_toBottomOf="@id/align"/>
            <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
                android:id="@id/lyt_message_group_map_friend_marker_avatar"
                android:layout_width="31dp"
                android:layout_height="31dp"
                android:layout_marginTop="1dp"
                app:layout_constraintTop_toTopOf="@id/lyt_message_group_map_friend_marker"
                app:layout_constraintLeft_toLeftOf="@id/lyt_message_group_map_friend_marker"
                app:layout_constraintRight_toRightOf="@id/lyt_message_group_map_friend_marker"/>
            <View
                android:id="@id/lyt_message_group_map_friend_mask"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:background="@drawable/message_friend_map_mask"
                app:layout_constraintRight_toRightOf="parent"/>
            <TextView
                android:id="@id/lyt_message_group_map_friend_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:paddingLeft="10dp"
                android:paddingBottom="2dp"
                android:paddingRight="10dp"
                android:textSize="12sp"
                android:textColor="@color/colorWhite"
                android:background="@drawable/img_rounded_group_number"
                app:layout_constraintLeft_toLeftOf="@id/lyt_message_group_map_friend_data"
                app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_data"/>
        </android.support.constraint.ConstraintLayout>
    </FrameLayout>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_friend_seen1"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_friend_seen2"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_friend_seen2"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_friend_seen3"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_friend_seen3"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_friend_seen4"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_friend_seen4"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_friend_seen5"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_friend_seen5"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_friend_seen6"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_friend_seen6"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_friend_seen7"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_friend_seen7"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_friend_seen8"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_friend_seen8"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_friend_seen9"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_friend_seen9"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_friend_more"/>
    <TextView
        android:id="@+id/lyt_message_group_map_friend_more"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:textSize="10sp"
        android:gravity="center"
        android:background="@drawable/img_rounded_group_number"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_friend_bound_content"
        app:layout_constraintRight_toRightOf="parent"/>
</android.support.constraint.ConstraintLayout>