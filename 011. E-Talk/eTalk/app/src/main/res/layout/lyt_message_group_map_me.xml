<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@id/lyt_message_group_map_me_row"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <FrameLayout
        android:id="@id/lyt_message_group_map_me_bound_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dp">
        <android.support.constraint.ConstraintLayout
            android:id="@id/lyt_message_group_map_me_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right">
            <com.example.cpu11398_local.etalk.presentation.custom.RoundedMapView
                android:id="@id/lyt_message_group_map_me_data"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginRight="25dp"
                app:layout_constraintRight_toRightOf="parent"/>
            <View
                android:id="@id/lyt_message_group_map_me_mask"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginRight="25dp"
                app:layout_constraintRight_toRightOf="parent"/>
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/ic_location_chooser"
                app:layout_constraintTop_toTopOf="@id/lyt_message_group_map_me_data"
                app:layout_constraintLeft_toLeftOf="@id/lyt_message_group_map_me_data"
                app:layout_constraintRight_toRightOf="@id/lyt_message_group_map_me_data"
                app:layout_constraintBottom_toBottomOf="@id/lyt_message_group_map_me_data"/>
            <TextView
                android:id="@id/lyt_message_group_map_me_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:paddingLeft="10dp"
                android:paddingBottom="2dp"
                android:paddingRight="10dp"
                android:textSize="12sp"
                android:textColor="@color/colorWhite"
                android:background="@drawable/img_rounded_group_number"
                app:layout_constraintLeft_toLeftOf="@id/lyt_message_group_map_me_data"
                app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_me_data"/>
            <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
                android:id="@id/lyt_message_group_map_me_status"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginRight="5dp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="@id/lyt_message_group_map_me_data"/>
        </android.support.constraint.ConstraintLayout>
    </FrameLayout>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_me_seen1"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_me_seen2"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_me_seen2"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_me_seen3"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_me_seen3"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_me_seen4"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_me_seen4"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_me_seen5"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_me_seen5"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_me_seen6"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_me_seen6"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_me_seen7"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_me_seen7"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_me_seen8"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_me_seen8"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_me_seen9"/>
    <com.example.cpu11398_local.etalk.presentation.custom.AvatarImageView
        android:id="@+id/lyt_message_group_map_me_seen9"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_me_bound_content"
        app:layout_constraintRight_toLeftOf="@id/lyt_message_group_map_me_more"/>
    <TextView
        android:id="@+id/lyt_message_group_map_me_more"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:textSize="10sp"
        android:gravity="center"
        android:background="@drawable/img_rounded_group_number"
        app:layout_constraintTop_toBottomOf="@id/lyt_message_group_map_me_bound_content"
        app:layout_constraintRight_toRightOf="parent"/>
</android.support.constraint.ConstraintLayout>